class Solution(object):
    def nextGreaterElements(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        stack=[]
        max_n=[-1]*len(nums)
        for i in range(len(nums)):
            
            while stack and nums[stack[-1]]<nums[i]:
              
                max_n[stack[-1]]=nums[i]
                stack.pop()
               
            stack.append(i)
        return max_n
