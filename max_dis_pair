class Solution(object):
    def maxDistance(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: int
        """
        max_d=0
        i=j=0
        while i<len(nums1) and j<len(nums2):
            if nums1[i]<=nums2[j]:
                max_d=max(max_d, j-i)
                j+=1
            else:
                i+=1
        return max_d
