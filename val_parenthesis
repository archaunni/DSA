class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        list_s=list(s)
        len_s=len(list_s)
        stack=[]
        push_c=0
       
        if len_s==1:
                return False
        else:
            for i in range(len_s):
                if list_s[i] in ['(','{','[']:
                    stack.append(list_s[i])
                    push_c+=1
                else:
                    
                    po_e=stack.pop() if stack else '#'
                    
                    if list_s[i] == ')' and po_e=='(':
                        push_c-=1
                        continue
                    elif list_s[i] == '}' and po_e=='{':
                        push_c-=1
                        continue
                    elif list_s[i] == ']' and po_e=='[':
                        push_c-=1
                        continue
                    else:
                       return False
        
            if push_c==0:
                return True
            else:
                return False
        
