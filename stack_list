def top(stack):
    if len(stack)>0:
        return stack[-1]
    else:
        return None
        
def pop(stack):
    if is_empty(stack) == 0:
        print("underflow")
        return None
    else:
        return stack.pop()
def is_empty(stack):
    if len(stack)==0:
        return 0
    else:
        return 1
def push(stack, item):
    stack.append(item)
print("stack using list")
stack=[]
push(stack,5)
print(stack)
print(stack)
print("top element",top(stack))
print("popped element",pop(stack))
print(stack)
